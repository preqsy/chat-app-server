input AuthUserCreate {
  FirstName: String!
  LastName: String!
  Email: String!
  Password: String!
}

type AuthUserResponse {
  Id: String
  FirstName: String!
  LastName: String!
  Email: String!
  TimeCreated: String!
  TimeUpdated: String!
}

type Mutation {
  createAuthUser(input: AuthUserCreate!): AuthUserResponse!
}
